{
    "name": "orajo/sms-zilla",
    "description": "PHP library for sending SMS through various gateways",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "sms",
        "library",
        "SerwerSMS",
        "Clickatell",
        "SMSApi",
        "SmsCenter",
        "Infobip",
        "Cisco",
        "Orange"
    ],
    "homepage": "https://github.com/Orajo/sms-zilla",
    "authors": [
        {
            "name": "Jaros≈Çaw Wasilewski",
            "email": "orajo@windowslive.com",
            "role": "Developer"
        }
    ],
    "require": {
        "arcturial/clickatell": "3.*",
        "smsapi.pl/php-client": "^1.7",
        "mlebkowski/mobitex": "1.0.*",
        "giggsey/libphonenumber-for-php": "8.*",
        "laminas/laminas-http": "2.*",
        "serwersms/serwersms-php-client": "1.*",
        "laminas/laminas-dependency-plugin": "^2.1",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "5.*"
    },
    "autoload": {
        "psr-0": {
            "SmsZilla\\": "src/"
        },
        "psr-4": {
            "SmsZilla\\": "src/SmsZilla/"
        },
        "classmap": [
            "src/"
        ]
    },
    "config": {
        "allow-plugins": {
            "laminas/laminas-dependency-plugin": true
        }
    }
}
